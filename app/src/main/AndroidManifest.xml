<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vicky.red.redpackets">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SurfaceActivity"
            android:label="@string/title_activity_surface"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BezierActivity"
            android:label="@string/title_activity_bezier"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BezierSurfaceActivity"
            android:label="@string/title_activity_bezier_surface"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>