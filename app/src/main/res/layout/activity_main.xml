<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.vicky.red.redpackets.MainActivity">


    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/btn_bezier"
        android:text="start bezier activity"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/btn_surfaceview"
        android:text="start surface activity"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/btn_bezier_surfaceview"
        android:text="start bezier surface activity"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/btn_meteor_surfaceview"
        android:text="start meteor surface activity"/>



</LinearLayout>
